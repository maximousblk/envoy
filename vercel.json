{
  "version": 2,
  "redirects": [
    {
      "source": "/.well-known/deno-import-intellisense.json",
      "destination": "https://raw.githubusercontent.com/maximousblk/envoy/master/src/public/.well-known/deno-import-intellisense.json"
    }
  ],
  "rewrites": [
    {
      "source": "/github/([^/]+)/(.+)",
      "destination": "/api/github?slug=$1/$2"
    },
    {
      "source": "/gitlab/([^/]+)/(.+)",
      "destination": "/api/gitlab?slug=$1/$2"
    },
    {
      "source": "/bitbucket/([^/]+)/(.+)",
      "destination": "/api/bitbucket?slug=$1/$2"
    },
    {
      "source": "/gist/(.+)",
      "destination": "/api/gist?slug=$1"
    },
    {
      "source": "/nest/(.+)",
      "destination": "/api/nest?slug=$1"
    },
    {
      "source": "/x/(.+)",
      "destination": "/api/x?slug=$1"
    },
    {
      "source": "/std/(.+)",
      "destination": "/api/std?slug=$1"
    }
  ]
}
